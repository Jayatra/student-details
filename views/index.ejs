<!--include header-->
<%- include('include/_header')%>
<!--/include header-->

<!--Main section-->
<main id="site-main">
    <div class="container">
        <div class="box-nave d-flex justify-between">
            <a href="/add-user" class="border-shadow">
            <span class="text-gradient">New Student
                    <i class="fa fa-user" aria-hidden="true"></i>
                </span>
            </a>
        </div>
        <!--form handing-->
        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>Sno.</th>
                        <th>Name</th>
                        <th>Contact</th>
                        <th>Subject</th>
                        <th>Class</th>
                        <th>Society</th>
                        <th>Year</th>
                        <th>Action</th>
    
                    </tr>
                </thead>
                <tbody>
                    <%- include('include/_show')%>
                </tbody>
            </table>
        </form>
    </div>
</main>

<%- include('include/_footer')%>